#Replace copies to symlinks in subsubdirectories
for i in */*/*; do
pushd $i
fdupes -q . > dupes.txt
sed -i 's|\./||g' dupes.txt
r=""; k=""
while read LINE; do
if [ "$k" = "" ]; then r=$LINE; else if [ "$LINE" != "" ]; then ln -sf $r $LINE; fi; fi
k=$LINE
done < dupes.txt
rm -f dupes.txt
popd
done

#Replace copies to symlinks in subdirectories
for i in */*; do
pushd $i
fdupes -qr . > dupes.txt
sed -i 's|\./||g' dupes.txt
r=""; k=""
while read LINE; do
if [ "$k" = "" ]; then r=$LINE; else if [ "$LINE" != "" ]; then ln -sf ../$r $LINE; fi; fi
k=$LINE
done < dupes.txt
rm -f dupes.txt
popd
done

#Replace copies to symlinks in directories
for i in *; do
pushd $i
fdupes -qr . > dupes.txt
sed -i 's|\./||g' dupes.txt
r=""; k=""
while read LINE; do
if [ "$k" = "" ]; then r=$LINE; else if [ "$LINE" != "" ]; then ln -sf ../../$r $LINE; fi; fi
k=$LINE
done < dupes.txt
rm -f dupes.txt
popd
done

